<ul class="pagination-custom">
  {% if paginate.previous %}
    <li>
      <a href="{{ paginate.previous.url }}" title="{{ paginate.previous.title }}">
        <span class="arrow-left"></span>
      </a>
    </li>
  {% else %}
    <li class="disabled"><span class="arrow-paginate arrow-left"></span></li>
  {% endif %}

  {% for part in paginate.parts %}
    {% if part.is_link %}
      <li>
        <a href="{{ part.url }}" title="">{{ part.title }}</a>
      </li>
    {% else %}
      {% if part.title == paginate.current_page %}
        <li class="active"><span>{{ part.title }}</span></li>
      {% else %}
        <li><span>{{ part.title }}</span></li>
      {% endif %}
    {% endif %}
  {% endfor %}

  {% if paginate.next %}
    <li>
      <a href="{{ paginate.next.url }}" title="{{ paginate.next.title }}">
        <span class="arrow-right"></span>
      </a>
    </li>
  {% else %}
    <li class="disabled"><span class="arrow-paginate arrow-right"></span></li>
  {% endif %}
</ul>