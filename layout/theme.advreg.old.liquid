  {% unless template contains 'index' %}
    <div id="back-to-top" class="fixed right-8 md:right-12 lg:right-16 bottom-36 cursor-pointer z-50">
      <span class="block flex justify-center items-center bg-white rounded-full w-10 h-10 text-black hover:bg-black hover:text-white">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6 transform transition hover:w-7 h-7" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 15l7-7 7 7" />
        </svg>
      </span>
    </div>
    <script type="text/javascript">
      $('#back-to-top').on("click", function () {
        $('html, body').animate({
          scrollTop: 0
        }, 'slow');
      });
      function showHidebackToTop() {
        if ($(window).scrollTop() > $(window).height() / 2) {
          $("#back-to-top").removeClass('hidden').addClass('block');
        } else {
          $("#back-to-top").removeClass('block').addClass('hidden');
        }
      }

      $(window).scroll(function () {
        showHidebackToTop();
      });
    </script>
