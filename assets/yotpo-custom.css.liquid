    display: none !important;
  }

  .yotpo .yotpo-bottomline .write-question-button .yotpo-icon.yotpo-icon-double-bubble {
    display: inline-block !important
  }

  .yotpo-dropdown-wrapper.mobile-collapse+.yotpo-clr {
    display: none !important;
  }

  .yotpo.yotpo-main-widget .yotpo-bottomline .yotpo-sum-reviews {
    padding: 0;
  }

  .yotpo.yotpo-main-widget .yotpo-bottomline .yotpo-icon-stars {
    vertical-align: middle;
  }

  .yotpo.yotpo-main-widget .yotpo-bottomline .yotpo-icon-star,
  .yotpo.yotpo-main-widget .yotpo-bottomline .yotpo-icon-half-star {
    font-size: 20px;
  }

  .yotpo-menu-mobile-collapse {
    display: flex !important;
    // position: absolute;
    // top: -130px;
    align-items: center;
    border: none !important;
    justify-content: space-between;
  }

  .yotpo.yotpo-main-widget.yotpo-small .yotpo-icon-btn-big.transparent-color-btn.yotpo-visible-mobile {
    display: none !important
  }

  .yotpo.yotpo-main-widget.yotpo-small .yotpo-bottomline.yotpo-bottomline-2-boxes .yotpo-bottomline-box-2 {
    // padding-bottom: 100px !important;
  }
  

  .yotpo.yotpo-main-widget.yotpo-small .yotpo-nav .yotpo-dropdown-wrapper.mobile-collapse .yotpo-menu-mobile-collapse .yotpo-nav-dropdown-category {
    display: block !important;
    background: #000 !important;
    color: #fff !important;
    margin-top: 10px;
    padding: 10px;
    width: 48%;
  }

  .yotpo.yotpo-main-widget.yotpo-small .yotpo-nav .yotpo-dropdown-wrapper.mobile-collapse .yotpo-menu-mobile-collapse .yotpo-nav-dropdown-category .yotpo-icon,
  .yotpo.yotpo-main-widget.yotpo-small .yotpo-nav .yotpo-dropdown-wrapper.mobile-collapse .yotpo-menu-mobile-collapse .yotpo-nav-dropdown-category .y-label {
