/* yotpo css */
@font-face {
  font-family: 'yotpo-widget-font';
  src: url("//staticw2.yotpo.com/assets/yotpo-widget-font.eot?version=2017-04-19_04-49-23");
  src: url("//staticw2.yotpo.com/assets/yotpo-widget-font.eot?version=2017-04-19_04-49-23#iefix") format("embedded-opentype"),
    url("//staticw2.yotpo.com/assets/yotpo-widget-font.woff?version=2017-04-19_04-49-23") format("woff"),
    url("//staticw2.yotpo.com/assets/yotpo-widget-font.ttf?version=2017-04-19_04-49-23") format("truetype"),
    url("//staticw2.yotpo.com/assets/yotpo-widget-font.svg?version=2017-04-19_04-49-23") format("svg");
  font-weight: normal;
  font-style: normal
}

@media screen and (-webkit-min-device-pixel-ratio: 0) {
  @font-face {
    font-family: 'yotpo-widget-font';
    src: url("//staticw2.yotpo.com/assets/yotpo-widget-font.svg?version=2017-04-19_04-49-23") format("svg")
  }
}

.yotpo-icon-star:before {
  font-family: "yotpo-widget-font" !important;
  content: "\e60e"
}

.yotpo .yotpo-bottomline .yotpo-icon-star,
.yotpo .yotpo-bottomline .yotpo-icon-half-star,
.yotpo .yotpo-bottomline .yotpo-icon-empty-star {
  color: #0BBCD4 !important;
  font-size: 0.875rem !important;
}

.yotpo .font-color-gray {
  color: #fff !important;
}

.yotpo-pictures-gallery-images-wrapper {
  display: flex;
  justify-content: center
}

.yotpo .yotpo-comment .yotpo-header.yotpo-store-owner .yopto-avatar-wrapper, 
.yotpo .yotpo-question .yotpo-header.yotpo-store-owner .yopto-avatar-wrapper{
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}

.product-single__prices,
.yotpo div,
.yotpo span,
.yotpo p,
.yotpo a,
.yotpo img,
.yotpo i,
.yotpo strong,
.yotpo sup,
.yotpo ul,
.yotpo li,
.yotpo form,
.yotpo label {
  font-family: 'Montserrat', sans-serif !important;
  color: #fff;
}

.yotpo-nav.yotpo-small-box .yotpo-nav>.yotpo-clr {
  display: none !important;
}

.yotpo a,
.yotpo a:hover,
.yotpo-action .yotpo-icon,
.yotpo-action:hover .yotpo-icon {
  color: #fff !important;
}

.yotpo .transparent-color-btn span.yotpo-icon {
  color: #fff !important;
}


@media screen and (max-width:1024px) {
