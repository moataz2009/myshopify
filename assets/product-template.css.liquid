  }

  .product-single__thumbnails::before,
  .product-single__thumbnails::after {
    content: '';
    height: 30px;
    width: 100%;
    background: rgba(255, 255, 255, 0.3);
    position: absolute;
    left: 0;
    visibility: hidden;
  }

  .product-single__thumbnails::before {
    top: 0;
  }

  .product-single__thumbnails::after {
    bottom: 0;
  }

  .product-single__thumbnails .grid__item {
