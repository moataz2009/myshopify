/* modal css */
.modal-mask {
    display: none;
    width: 100%;
    height: 100%;
    position: absolute;
    background-color: rgba(0, 0, 0, 0.6);
    left: 0;
    top: 0;
    z-index: 99999;
}

.pt-modal-container {
    width: 100vw;
    height: 100vh;
    /* transform: scale(0); */
    /* transform-origin: center center; */
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100001;
    visibility: hidden;
}

.pt-modal-container .modal-background {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.8);
    display: flex;
    justify-content: center;
    align-items: center;
}

.pt-modal-container.open {
    /* transform: scaleY(.01) scaleX(0); */
    /* animation: unfoldIn 1s cubic-bezier(0.165, 0.840, 0.440, 1.000) forwards; */
    visibility: visible;
}

.pt-modal-container.out {
    visibility: hidden;
    /* transform: scale(1); */
    /* animation: unfoldOut 1s .3s cubic-bezier(0.165, 0.840, 0.440, 1.000) forwards; */
}

.pt-modal-container .pt-modal-dialog {
    width: 80vw;
    max-width: 1280px;
}

/* .pt-modal-container.open .pt-modal-dialog {
    transform: scale(0);
    animation: zoomIn .5s .8s cubic-bezier(0.165, 0.840, 0.440, 1.000) forwards;
  }
  
  .pt-modal-container.out .pt-modal-dialog {
    animation: zoomOut .5s cubic-bezier(0.165, 0.840, 0.440, 1.000) forwards;
  } */

.pt-modal-container video,
.pt-modal-container picture {
    max-width: 100%;
    height: auto;
}

.pt-modal-close {
    right: -20px;
    top: -20px;
    font-size: 22px;
    background: #fff;
    border-radius: 50%;
    width: 36px;
    height: 36px;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
}

.pt-modal-close span {
    color: #000;
    line-height: 1;
}

/* modal video */
.play-btn img {
    display: inline-block !important;
    vertical-align: bottom;
}

@keyframes unfoldIn {
    0% {
        transform: scaleY(0.005) scaleX(0);
    }

    50% {
        transform: scaleY(0.005) scaleX(1);
    }

    100% {
        transform: scaleY(1) scaleX(1);
    }
}

@keyframes unfoldOut {
    0% {
        transform: scaleY(1) scaleX(1);
    }

    50% {
        transform: scaleY(0.005) scaleX(1);
    }

    100% {
        transform: scaleY(0.005) scaleX(0);
    }
}

@keyframes zoomIn {
    0% {
        transform: scale(0);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes zoomOut {
    0% {
        transform: scale(1);
    }

    100% {
        transform: scale(0);
    }
}

