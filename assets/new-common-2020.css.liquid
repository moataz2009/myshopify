}

.pt-text-gradient::before {
    content: attr(text);
    position: absolute;
    z-index: 10;
    color: rgb(2, 253, 254);
    -webkit-mask: linear-gradient(
        91deg,
        transparent,
        rgb(2, 253, 254) 46%,
        transparent
    );
}

.pt-line-btn {
    display: block;
    margin: 0 auto;
    background-color: transparent;
    position: relative;
    width: max-content;
    line-height: 40px;
    border: 1px solid transparent;
    overflow: hidden;
    padding: 0 35px;
    cursor: url("//cdn.shopify.com/s/files/1/0515/2342/0337/t/2/assets/cursor-arrow.png?v=18350407043478802101"),
        default !important;
}

.pt-line-btn::before {
    content: "";
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(
        120deg,
        transparent,
        rgba(31, 85, 240, 0.3),
        transparent
    );
    transition: all 650ms;
}

.pt-line-btn:hover::before {
    left: 100%;
}

.pt-line-btn:hover {
    border: 1px solid #3ee1ff;
    box-shadow: 1px 1px 25px 8px #1f55f0;
}

.pt-line-btn:hover .border-lt,
.pt-line-btn:hover .border-rt,
.pt-line-btn:hover .border-lb,
.pt-line-btn:hover .border-rb {
    opacity: 0;
}

.pt-line-btn .border-lt,
.pt-line-btn .border-rt,
.pt-line-btn .border-lb,
.pt-line-btn .border-rb {
    width: 20px;
    height: 12px;
    border-color: #fff;
    border-style: solid;
    border-width: 0;
    position: absolute;
}

.pt-line-btn .border-lt {
    border-top-width: 1px;
    border-left-width: 1px;
    left: 0;
    top: 0;
}

.pt-line-btn .border-rt {
    border-top-width: 1px;
    border-right-width: 1px;
    right: 0;
    top: 0;
}

.pt-line-btn .border-rb {
    border-bottom-width: 1px;
    border-right-width: 1px;
    right: 0;
    bottom: 0;
}

.pt-line-btn .border-lb {
    border-bottom-width: 1px;
    border-left-width: 1px;
    left: 0;
    bottom: 0;
}

.pt-flex-between .pt-line-btn {
