    margin: 0 auto;
    width: 100%;
    max-width: none;
  }

  .container h1 {
    font-size: 2.25em;
    font-weight: 800;
  }

  .container form {
    text-align: center;
    width: 28em;
    position: relative;
    margin-left: auto;
    margin-right: auto;
  }


