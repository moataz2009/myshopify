<script>
  $(function () {
    $(".toggle-btns").on("click", "img", function () {
      var toggle_target = $(this).data('target');
      var this_src = $(this).attr('src');
      if (this_src.indexOf('active') != -1) {
        return;
      } else {
        var other_src = $(this).siblings().attr('src');
        var this_src_name = this_src.split('.png');
        var other_src_name = other_src.split('-active.png');
        $(this).attr('src', this_src_name[0] + '-active.png').siblings().attr('src', other_src_name[0] +
          '.png');

        $(toggle_target).addClass('active').siblings().removeClass('active');
      }
    })
  })
</script>
