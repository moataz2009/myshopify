  .swap_line {
    max-width: unset;
    z-index: 9;
  }

  .swap_lightning,
  .swap_typec {
    width: 100%;
    z-index: 10;
  }

  .swap_typec {
    display: none;
  }

  .swap_iphone,
  .swap_console,
  .swap_typec,
  .swap_samsung,
  .swap_game {
    display: none;
    z-index: 11;
  }

  /* toggle bg */
  .toggle-sec {
    z-index: 10;
  }

  .toggle-sec .txtCont {
    top: 5em;
  }

  .toggle-sec .toggle-btns img {
    margin: 3em 2em 0;
  }

  .toggle-bg {
    display: none;
  }

  .toggle-bg.active {
    display: block;
  }

  /* pad-sec */
  .pad-sec {
    padding: 5em 0;
  }

  .pad-sec picture {
    margin-top: 5em;
  }


  /* video-sec */
  .video-sec {
    padding: 5em 7%;
  }

  .video-sec video {
    width: 60%;
    height: auto;
  }

  .video-sec .video-content {
    width: 40%;
    padding-left: 5%;
  }

  /* side-sec */
  .side-sec {
    padding: 5em 7%;
  }

  .side-sec .txtCont {
    width: 40%;
    padding-right: 5%;
  }

  .side-sec picture {
    width: 60%;
  }

  /* direction-sec */
  .direction-sec {
    background: url("{{ 'air-omni-direction-bg.jpg' | asset_url }}") center 0;
    background-size: cover;
    padding-top: 5em;
    height: 100vh;
    overflow: hidden;
  }

  .direction-sec .anim-container {
    display: flex;
    justify-content: center;
    align-items: flex-end;
    max-width: 40%;
  }

  .direction-sec .txtCont {
    margin-bottom: 5em;
  }

  #anim-ipad-1,
  #anim-ipad-2 {
    z-index: 10;
  }

  .direction-base {
    z-index: 9;
  }

  #anim-cable {
    z-index: 8;
  }

  #anim-holder {
    z-index: 11;
  }

  /* need section */
  .need-sec .txtCont {
    width: 40%;
    padding-left: 7%;
  }

  .media-sec {
    padding: 5em 0;
  }

  /* 6 - new order section */
  .order-sec .txtCont {
    width: 40%;
    padding-left: 7%;
  }

  @media screen and (min-width:768px) {
    .pc_tablet_dspFlex_row {
      display: flex;
      flex-flow: row nowrap;
    }

    .pc_tablet_height100 {
      height: 100%;
    }

    .pc_tablet_height50 {
      height: 50%;
    }
  }

  @media screen and (min-width:1280px) {
    .pc_dspFlex_row {
      display: flex;
      flex-flow: row nowrap;
    }

    .pc_dspFlex_column {
      display: flex;
      flex-flow: column nowrap;
    }

    .pc_transY_center {
      top: 50%;
      transform: translateY(-50%);
    }

    .pc_txt_center {
      text-align: center;
    }

    .pc_min_vh100 {
      min-height: 100vh;
      height: auto
    }

    .pc_vh100 {
      height: 100vh;
    }

    .pc_height100 {
      height: 100%;
    }

    .pc_height50 {
      height: 50%;
    }

    .pc_wid-full {
      width: 100%;
    }

    .pc_wid-75 {
      width: 75%;
    }

    /* hero */
    .hero .absolute {
      width: 40%;
      max-width: 600px;
    }

    .hero .shop-btn {
      display: inline-block;
    }

    /* swap animation */
    .swap-charging .txtCont {
      width: 40%;
      padding-left: 8%;
      justify-content: center;
    }

    .swap-charging-img {
      width: 60%;
      display: flex;
      justify-content: center;
      align-items: flex-end;
      margin: 5% 2% 0;
    }
  }

  @media screen and (max-width:1700px) {
    .ft-48 {
      font-size: 2.5em;
    }

    .ft-24 {
      font-size: 1.3em;
    }

    .ft-18 {
      font-size: 1em;
    }

    .ft-16 {
      font-size: .9em;
    }

    /* hero */
    .hero .txtCont .detail-font-xmedium:last-of-type {
      max-width: 450px;
    }
  }

  @media screen and (max-width:1400px) {
    .ft-48 {
      font-size: 2em;
    }

    .ft-30 {
      font-size: 1.5em;
    }

    .ft-24 {
      font-size: 1.2em;
    }

    .ft-18 {
      font-size: 0.9em;
    }

    .ft-16 {
      font-size: .8em;
    }

    .direction-sec .anim-container {
      max-width: 50%;
    }
  }

  @media screen and (max-width:1279px) {
    .tablet_mobile_hide {
      display: none;
    }

    .tablet_mobile_dspFlex_row {
      display: flex;
      flex-flow: row nowrap;
    }

    .tablet_mobile_dspFlex_column {
      display: flex;
      flex-flow: column nowrap;
    }

    .tablet_mobile_transX_center {
      left: 50%;
      transform: translateX(-50%);
    }

    .tablet_mobile_txt_center {
      text-align: center;
    }

    .tablet_mobile_margin_center {
      margin-left: auto;
      margin-right: auto;
    }

    .tablet_mobile_margin_btm_0 {
      margin-bottom: 0;
    }

    .tablet_mobile_padding_top_btm_5 {
      padding: 5% 0;
    }

    .tablet_wid-80 {
      width: 80%;
    }

    .tablet_wid-70 {
      width: 70%;
    }

    /* hero */
    .hero .absolute {
      top: 5%;
      max-width: 450px;
    }

    /* swap animation */
    .swap-charging {
      height: 100vh;
      justify-content: space-between;
    }

    .swap-charging-img {
      display: flex;
      justify-content: center;
      align-items: center;
    }

    .swap-charging .txtCont {
      margin-top: 20%;
    }

    .swap_iphone,
    .swap_console,
    .swap_samsung,
    .swap_game {
      top: 0;
      left: 0;
    }

    .swap-charging .txtCont {
      /*       width:86%; */
      max-width: 82%;
      width: 45vh;
    }

    /* order section */
    .order-sec table {
      margin-bottom: 0;
    }

    .order-sec table td {
      width: 200px;
    }
  }

  @media screen and (max-width:1024px) {

    .video-sec,
    .side-sec,
    .pad-sec {
      padding: 4em 0;
    }

    .video-sec .video-content {
      order: 1;
      width: 100%;
      text-align: center;
      padding: 0 10% 4em;
    }

    .video-sec video {
      order: 2;
      width: 100%;
    }

    .side-sec .txtCont,
    .direction-sec .txtCont,
    .need-sec .txtCont {
      width: 100%;
      padding: 0 10% 4em;
      margin: 0;
    }

    .side-sec picture {
      width: 100%;
    }

    .specs-sec {
      padding: 4em 6%;
    }

    .hero .absolute {
      padding: 4em 0 3em;
      top: 0;
      height: 100%;
    }

    .direction-sec .anim-container {
      max-width: 80%;
    }

    .swap-charging,
    .direction-sec {
      justify-content: flex-end !important;
    }
  }

  @media screen and (max-width:767px) {
    .mobile_hide {
      display: none;
    }

    .mobile_transX_center {
      left: 50%;
      transform: translateX(-50%);
    }

    .mobile_dspFlex_center {
      display: flex;
      justify-content: center;
      align-items: center;
    }

    .mobile_dspFlex_column {
      flex-flow: column;
    }

    .mobile_txt_center {
      text-align: center;
    }

    .mobile_margin_center {
      margin-left: auto;
      margin-right: auto;
    }

    .mobile_wid-full {
      width: 100%;
    }

    .mobile_height100 {
      height: 100%;
    }

    .mobile_relative {
      position: relative;
    }

    .mobile_absolute {
      position: absolute;
    }

    /*  hero  */
    .hero .absolute.txtCont {
      padding: 4em 7%;
      width: 100%;
    }

    /* shop button */
    .shop-btn .more-btn {
      height: max-content;
      line-height: 40px;
      width: 120px;
      font-size: 12px;
      padding: 0;
    }


    .toggle-sec .txtCont {
      top: 3em;
    }

    .toggle-sec .toggle-btns img {
      margin-top: 2em;
    }

    .pad-sec,
    .video-sec,
    .side-sec {
      padding: 3em 0;
    }

    .need-sec .txtCont {
      top: 3em;
      transform: none;
    }


    /* order sec */
    .order-sec .absolute {
      width: 100%;
      padding: 3em 7%;
      transform: none;
      position: static;
    }

    .order-sec .standard-h3 {
      margin-bottom: 8%;
    }

    .ft-48 {
      font-size: 1.5em;
    }

    .ft-30 {
      font-size: 1.2em;
      line-height: 1.2;
    }

    .ft-18 {
      font-size: 0.9em;
    }
  }
</style>

<!-- specs css -->
<style>
  .padding-updown {
    padding: 5em 0;
  }

  .specs-sec {
    background-color: #171719;
    padding: 5em 7%;
  }

  .dspFlex_wrap_evenly {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-evenly;
  }

  .features-piece {
    align-items: stretch;
  }

  .features-piece p {
    padding: 0 15%;
  }

  .features-piece:nth-child(2n) {
    padding-right: 7%
  }

  .features-piece:nth-child(2n+1) {
    padding-left: 7%
  }

  .features-piece img {
    width: 150px;
    display: block;
    margin: 0 auto;
  }

  .features-piece p a {
    color: #fff;
    font-weight: 500;
    font-style: italic;
    text-decoration: underline;
  }


  /* specs */
  .devices-list {
    margin: 5em 0 0;
  }

  .devices-container {
    display: flex;
    align-items: center;
    flex-direction: column;
    margin-bottom: 5em;
  }

  .devices-container img {
    display: block;
    margin: 3em auto 0;
  }

  .devices-grid {
    padding-right: 3em;
  }

  .devices-grid:first-child {
    width: 22%;
  }

  .devices-grid:nth-child(2) {
    width: 24%;
  }

  .devices-grid:nth-child(3) {
    width: 27%;
  }

  .devices-grid:nth-child(4) {
    width: 27%;
    padding: 0;
  }

  .devices-title {
    margin-bottom: 15px;
  }

  .devices-grid .ul p {
    margin-bottom: 0;
    color: #fff;
  }

  .devices-grid .ul p span {
    width: 330px;
    display: inline-block;
    margin-right: 15px;
  }

  .devices-grid .ul p.ft-18 {
    line-height: 1.75;
  }

  @media screen and (max-width:1440px) {
    .devices-container img {
      max-width: 100%
    }

    .devices-grid .ul p.ft-18 {
      font-size: 14px;
    }

    .devices-grid {
      padding-right: 1em;
    }

  }

  @media screen and (max-width:1024px) {
    .padding-updown {
      padding: 4em 7%;
    }

    .ft-30 {
      font-size: 1.25em;
    }

    .ft-24 {
      font-size: 1.05em;
    }

    .dspFlex_row {
      flex-wrap: wrap;
    }

    .devices-list {
      margin-top: 0;
    }

    .wid-half {
      width: 100%;
      padding: 0 10%;
    }

    .features-piece:nth-child(2n+1),
    .features-piece:nth-child(2n) {
      padding: 0 15%;
    }

    .devices-grid {
      margin-top: 1.5em;
    }

    .devices-container {
      margin-bottom: 4em;
    }

    .devices-container img {
      margin: 2em auto 0;
    }

    .devices-grid .ul p span {
      width: auto;
      display: initial;
      margin-right: 15px;
    }

    .devices-grid .ul p.ft-18 {
      font-size: 13px;
      line-height: 1.5;
    }
  }

  @media screen and (max-width:768px) {
    .txtCont {
      top: 20%;
    }

    p.detail-font-xmedium {
      line-height: 1.5;
    }

    .devices-grid {
      width: 50% !important;
    }

    .devices-grid .ul p.ft-18 {
      font-size: 12px;
    }

    .padding-updown {
      padding: 3em 10%;
    }

    .features-piece:nth-child(2n+1),
    .features-piece:nth-child(2n) {
      padding: 0;
    }
  }
</style>
<!-- specs css end -->

<!-- parallex scroll CSS -->
<style>
  .parallexScroll .section-picture {
    margin-top: 0;
  }

  .parallexScroll .section-picture img {
    object-fit: cover;
    float: none;
    display: block;
  }

  /* tablet and pc screens */
  @media screen and (min-width:768px) {
    /* .txtCont {
      top: 30%;
    } */

    /* parallex scroll */
    .parallexScroll {
      overflow: hidden;
    }

    .parallexScroll .section-picture {
      width: 60%;
      align-self: flex-start;
      min-width: 425px;
    }

    .parallexScroll .section-picture img {
      height: auto;
      width: 100%;
    }

    .parallexScroll .txtCont {
      text-align: left;
      width: 40%;
      padding: 0 5%;
      max-width: 750px;
    }
  }

  @media screen and (min-width:1280px) {

    /* parallex scroll */
    .parallexScroll .section-picture {
      min-width: 708px;
      width: 58%;
      /* max-width: 960px; */
    }

    .parallexScroll .txtCont {
      width: 42%;
      max-width: unset;
    }
  }

  @media screen and (max-width:1279px) and (min-width:768px) and (orientation: portrait) {

    /* parallex scroll */
    .parallexScroll {
      flex-flow: column-reverse nowrap;
      max-height: unset;
      height: auto;
    }

    .parallexScroll .txtCont {
      padding: 4em 0;
      width: 80%;
      max-width: 600px;
      height: 32%;
      text-align: center;
    }

    .parallexScroll .section-picture {
      height: 68%;
      width: 100%;
      overflow: visible;
    }
  }

  @media screen and (max-width:767px) {
    .parallexScroll {
      flex-flow: column-reverse nowrap;
    }

    .parallexScroll .txtCont {
      padding: 3em 10%;
      width: 100%;
      max-width: 400px;
    }

    .parallexScroll .section-picture {
      width: 100%;
      overflow: hidden;
    }
  }
</style>
<!-- end parallex scroll CSS -->

