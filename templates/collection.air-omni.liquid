    left: 50%;
    transform: translateX(-50%);
  }

  .transY_center {
    top: 50%;
    transform: translateY(-50%);
  }

  .txt_center {
    text-align: center;
  }

  .margin_center {
    margin-left: auto;
    margin-right: auto;
