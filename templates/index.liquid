<span class="hidden" id="openModal"></span>
{% section 'pop-modal' %}

{{'jquery.cookie.js' | asset_url | script_tag }}
<script>
  $(function () {
    $("#openModal").on('click', function () {
      //       console.log(123)
      $("#6th-modal").show();
      $('.6th-modal-cover').show();
      $.cookie("hasOpen", "yes", {
        expires: 1
      });
      //       console.log($.cookie("hasOpen"))
    })
    if ($.cookie("hasOpen") != 'yes') {
      $("#openModal").click();
    }
  })
</script>

