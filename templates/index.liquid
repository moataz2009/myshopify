<style>
  /* .multiple-slider {
    padding: 0 1.5rem 3em;
    margin: 0 -1.5rem;
  } */
  .multiple-slider .swiper-slide {
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
  }

  .multiple-slider>div[role="button"] {
    top: 45%;
  }

  .multiple-slider>div[role="button"],
  .gallery-top>div[role="button"] {
    width: 12px;
    height: 16px;
  }

  .multiple-slider>div[role="button"]:after,
  .gallery-top>div[role="button"]:after {
    content: '';
    width: 100%;
    height: 100%;
    background: url({{'triggle_pixel.svg' | asset_url}}) no-repeat;
    background-size: contain;
  }

  .gallery-top .swiper-button-prev {
    transform: rotate(-180deg);
    left: 0;
  }

  .gallery-top .swiper-button-next {
    right: 0;
  }

  .multiple-slider .swiper-button-next {
    right: 0;
  }

  .multiple-slider .swiper-button-prev {
    left: 0;
    transform: rotate(-180deg);
  }

  .multiple-slider .swiper-pagination-bullets {
    display: none;
  }

  .multiple-slider .square-bullet {
    width: 8px;
    height: 8px;
    background-color: #565557;
    display: block;
    margin: 0 .4rem;
  }

  .multiple-slider .swiper-pagination-bullet-active {
    background-color: #fff;
  }

  .gallery-top {
    height: 80%;
    width: 100%;
  }

  .gallery-top .swiper-slide {
    padding: 0 1.5rem;
  }

  .gallery-thumbs {
    height: 45px;
    box-sizing: content-box;
    padding-top: 60px;
  }

  .gallery-thumbs .swiper-slide {
    width: 20%;
    height: 100%;
    opacity: 0.4;
    background-size: contain;
    background-position: center;
    background-repeat: no-repeat;
  }

  .gallery-thumbs .swiper-slide-thumb-active {
    opacity: 1;
  }

  @media screen and (min-width:768px) {

    .multiple-slider .swiper-slide {
      min-height: 586px;
    }

    .multiple-slider {
      padding: 0;
      margin: 0;
      padding-bottom: 4rem;
    }

    .multiple-slider>div[role="button"] {
      top: unset;
      bottom: 6px;
      margin: 0;
    }

    .multiple-slider .swiper-button-next {
      right: calc(50% - 153.2px);
    }

    .multiple-slider .swiper-button-prev {
      left: calc(50% - 153.2px);
    }

    .multiple-slider .swiper-pagination-bullets {
      display: flex;
      justify-content: center;
      margin: 0 -0.4rem;
    }

    .gallery-top .swiper-slide {
      padding: 0;
    }

    .gallery-thumbs {
      padding-bottom: 36px;
    }
  }

  @media screen and (min-width:1025px) {
    .gallery-thumbs {
      padding: 90px 0 42px;
    }
  }

  @media screen and (min-width:1280px) {
    .multiple-slider .swiper-slide {
      min-height: 586px;
    }

    .gallery-thumbs {
      padding: 120px 0 54px;
    }

