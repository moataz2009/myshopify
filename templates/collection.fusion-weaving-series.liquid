      slideChangeTransitionEnd: function () {
        console.log("enter");
        let swiperVideo = $('.swiperVideo')
        let playVideo = $('.swiper-slide-active div video')
        swiperVideo[0].pause()
        swiperVideo[1].pause()
        swiperVideo[2].pause()
        swiperVideo[3].pause()
        swiperVideo[4].pause()
        if (playVideo[0]) {
          playVideo[0].play()
        }
      },
    },
  });
</script>

